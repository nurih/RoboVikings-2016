<?xml version='1.0' encoding='UTF-8' standalone='yes' ?>
<Robot type="FirstInspires-FTC">
    <MotorController name="Motor Controller 1" serialNumber="A104ORCS">
        <Motor name="lmotor" port="1" />
        <Motor name="rmotor" port="2" />
    </MotorController>
    <MotorController name="Motor Controller 2" serialNumber="A104OQ3R">
        <Motor name="forkliftmotor" port="1" />
        <Motor name="windermotor" port="2" />
    </MotorController>
    <!-- elevatormotor  needs extra motor controller! -->
    <DeviceInterfaceModule name="Device Interface Module 1" serialNumber="AL00VCH6">
        <ColorSensor name="colorsensor" port="0" />
        <I2cDevice name="mr" port="1" />
    </DeviceInterfaceModule>

    <ServoController name="Servo Controller 1" serialNumber="AL00VTN6">
        <Servo name="lflipperservo" port="1" />
        <Servo name="rflipperservo" port="2" />
        <Servo name="scoopservo" port="3" />
    </ServoController>
</Robot>


